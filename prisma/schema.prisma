
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Banner {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  description   String
  cta String
  image String
  background  String
  type String
}
